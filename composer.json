{
  "require": {
    "php": "^8.1",
    "ext-mbstring": "*",
    "ext-gd": "*",
    "ext-dom": "*",
    "ext-iconv": "*",
    "dompdf/dompdf": "2.0.8",
    "freemius/wordpress-sdk": "^2.11",
    "youniwemi/digital-invoice": "^0.2.2"
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/Youniwemi/zugferd-php"
    }
  ],
  "scripts": {
    "cs": "phpcs -s --standard=phpcs.xml .",
    "cbf": "phpcbf --standard=phpcs.xml .",
    "qit": "qit run:e2e einvoicing-pro-for-woocommerce"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "woocommerce/qit-cli": "^0.8.6"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
